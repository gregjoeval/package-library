{
  "name": "@gjv/eslint-config-typescript",
  "version": "0.10.1",
  "description": "gregjoeval's ESLint config for TS",
  "author": "Greg Valentine <gregjoeval@gmail.com>",
  "homepage": "https://github.com/gregjoeval/package-library#readme",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "setup:install": "npm install",
    "setup:clean-install": "npm ci",
    "setup:update": "npx npm-check -u",
    "setup:clean": "rimraf ./node_modules",
    "lint": "eslint ./index.js --ext .js,.jsx,.ts,.tsx  --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:config": "eslint --print-config ./index.js",
    "check": "npm run lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-promise": "^6.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "~5",
    "@typescript-eslint/parser": "~5",
    "eslint": "~8",
    "typescript": "~4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregjoeval/package-library.git"
  },
  "bugs": {
    "url": "https://github.com/gregjoeval/package-library/issues"
  },
  "gitHead": "4c1a9bcc8e7d5de4ad16c947613e634fd79ca534"
}
