{
  "name": "@gjv/eslint-config-typescript",
  "version": "0.7.0",
  "description": "gregjoeval's ESLint config for TS",
  "author": "Greg Valentine <gregjoeval@gmail.com>",
  "homepage": "https://github.com/gregjoeval/package-library#readme",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "setup:install": "npm install",
    "setup:clean-install": "npm ci",
    "setup:clean": "rimraf ./node_modules",
    "lint": "eslint ./index.js --ext .js,.jsx,.ts,.tsx  --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:config": "eslint --print-config ./index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-promise": "^4.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "~4",
    "@typescript-eslint/parser": "~4",
    "eslint": "~7",
    "typescript": "~4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregjoeval/package-library.git"
  },
  "bugs": {
    "url": "https://github.com/gregjoeval/package-library/issues"
  },
  "gitHead": "11f6564aac2f9b5cce45e4cbe01c3ce531297450"
}
