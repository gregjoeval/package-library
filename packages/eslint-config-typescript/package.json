{
  "name": "@gjv/eslint-config-typescript",
  "version": "0.5.7",
  "description": "gregjoeval's ESLint config for TS",
  "author": "Greg Valentine <gregjoeval@gmail.com>",
  "homepage": "https://github.com/gregjoeval/package-library#readme",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "setup:install": "npm install",
    "setup:clean-install": "npm ci",
    "setup:clean": "rimraf ./node_modules",
    "lint": "eslint ./index.js --ext .js,.jsx,.ts,.tsx  --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:config": "eslint --print-config ./index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-promise": "^4.2.1"
  },
  "devDependencies": {
    "@gjv/eslint-config": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "@gjv/eslint-config": "~0",
    "@typescript-eslint/eslint-plugin": "~2",
    "@typescript-eslint/parser": "~2",
    "eslint": "~6",
    "typescript": "~3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregjoeval/package-library.git"
  },
  "bugs": {
