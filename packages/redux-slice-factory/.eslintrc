{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true,
    "jasmine": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": [
    "eslint-plugin-tsdoc"
  ],
  "extends": [
    "@gjv/eslint-config-typescript"
  ],
  "rules": {
    "tsdoc/syntax": "warn"
  }
}
